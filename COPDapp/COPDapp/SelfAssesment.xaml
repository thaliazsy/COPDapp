<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="COPDapp.SelfAssesment" BackgroundColor="#dff4d7">
    <ContentPage.ToolbarItems>
        <ToolbarItem Name="Home" Order="Primary" Icon="home.png" Text="Home" Priority="0" Activated="Home_Activated" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <ScrollView>
            <StackLayout VerticalOptions="Center" Spacing="20">
                <Image x:Name="cat" Source="COPD_CAT" />
                <Image x:Name="mmrc" Source="COPD_mMRC" />
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>